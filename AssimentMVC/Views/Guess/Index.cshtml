
@{
    ViewData["Title"] = "GuessingGame";
}

<h1>GuessingGame</h1>

<form asp-controller="Guess" asp-action="Index" method="post">
    <input type="number" name="geussNumber" min="1" max="100" />
    <input type="submit" name="geussNumber" value="Tryck" />
</form>


<table class="table table-responsive">
    <tr>
        <th>Guess</th>
    </tr>
    <tr>
        <td>@ViewBag.Age</td>
    </tr>
</table>
<p>@ViewData["Message"]</p>

